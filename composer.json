{
    "name": "itpathsolutions/chatbot",
    "description": "Live chatting with chatbot",
    "type": "composer-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Itpathsolutions\\Chatbot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
           "Itpathsolutions\\Chatbot\\": "src/",
           "Tests\\": "tests/"
         }
     },
    "authors": [
        {
            "name": "Dhanik Keraliya"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.8",
        "illuminate/support": ">=5.4"
    },
    "extra": {
        "laravel":{
            "providers":[
                "Itpathsolutions\\Chatbot\\ChatbotServiceProvider"
            ]
        }
    }
}
